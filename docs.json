{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "WinWinKit",
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "colors": {
    "primary": "#111113",
    "light": "#D4FF70",
    "dark": "#111113"
  },
  "background": {
    "color": {
      "light": "#FCFCFD",
      "dark": "#111113"
    }
  },
  "logo": {
    "dark": "/logo.svg",
    "light": "/logo.svg",
    "href": "https://winwinkit.com"
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "General",
            "pages": ["general/introduction"]
          },
          {
            "group": "Features",
            "pages": [
              "features/affiliates",
              "features/referrals",
              "features/promos",
              {
                "group": "Rewards",
                "pages": [
                  "features/rewards/introduction",
                  "features/rewards/basic",
                  "features/rewards/credit",
                  "features/rewards/offer-code",
                  "features/rewards/revenuecat-entitlement",
                  "features/rewards/revenuecat-offering"
                ]
              },
              "features/users",
              {
                "group": "Pages",
                "pages": [
                  "features/pages/code-link",
                  "features/pages/affiliate-form"
                ]
              }
            ]
          },
          {
            "group": "Integrations",
            "pages": [
              "integrations/api-keys",
              "integrations/swift-sdk",
              "integrations/dart-sdk",
              "integrations/typescript-sdk"
            ]
          },
          {
            "group": "Misc",
            "pages": ["misc/glossary"]
          }
        ]
      },
      {
        "tab": "API Reference",
        "openapi": {
          "source": "https://api.winwinkit.com/openapi-yaml",
          "directory": "api-reference"
        },
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction",
              "api-reference/authentication",
              "api-reference/errors"
            ]
          }
        ]
      },
      {
        "tab": "Changelog",
        "groups": [
          {
            "group": "Changelog",
            "pages": ["changelog/product-updates"]
          }
        ]
      },
      {
        "tab": "Support",
        "groups": [
          {
            "group": "Support",
            "pages": ["support/contact-us"]
          }
        ]
      }
    ]
  },
  "api": {
    "playground": {
      "display": "interactive"
    },
    "examples": {
      "languages": ["curl", "javascript"]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "GitHub",
        "href": "https://github.com/winwinkit"
      },
      {
        "label": "Get Started",
        "href": "https://winwinkit.com"
      }
    ]
  },
  "footer": {
    "socials": {
      "website": "https://winwinkit.com",
      "reddit": "https://reddit.com/r/winwinkit",
      "linkedin": "https://linkedin.com/company/winwinkit",
      "bluesky": "https://bsky.app/profile/winwinkit.bsky.social",
      "x-twitter": "https://x.com/winwin_kit",
      "github": "https://github.com/winwinkit"
    }
  }
}

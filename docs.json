{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "WinWinKit",
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "colors": {
    "primary": "#111113",
    "light": "#D4FF70",
    "dark": "#111113"
  },
  "background": {
    "color": {
      "light": "#FCFCFD",
      "dark": "#111113"
    }
  },
  "logo": {
    "dark": "/logo.svg",
    "light": "/logo.svg",
    "href": "https://winwinkit.com"
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "General",
            "pages": [
              "introduction",
              "quickstart",
              "glossary"
            ]
          },
          {
            "group": "Referral",
            "pages": [
              "referral/rewards",
              "referral/programs",
              "referral/users",
              "referral/preview-link"
            ]
          },
          {
            "group": "Offer Codes",
            "pages": [
              "offer-codes/introduction",
              "offer-codes/create",
              "offer-codes/enable",
              "offer-codes/integrate"
            ]
          },
          {
            "group": "Integrations",
            "pages": [
              "integrations/api-keys",
              "integrations/swift-sdk",
              "integrations/typescript-sdk"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "openapi": {
          "source": "api-reference/openapi.yaml",
          "directory": "api-reference"
        },
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction",
              "api-reference/authentication",
              "api-reference/errors"
            ]
          }
        ]
      },
      {
        "tab": "Support",
        "groups": [
          {
            "group": "Support",
            "pages": [
              "support/contact-us"
            ]
          }
        ]
      }
    ]
  },
  "api": {
    "playground": {
      "display": "interactive"
    },
    "examples": {
      "languages": ["curl", "javascript"]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "GitHub",
        "href": "https://github.com/winwinkit"
      },
      {
        "label": "Get Started",
        "href": "https://winwinkit.com"
      }
    ]
  },
  "footer": {
    "socials": {
      "website": "https://winwinkit.com",
      "reddit": "https://reddit.com/r/winwinkit",
      "linkedin": "https://linkedin.com/company/winwinkit",
      "bluesky": "https://bsky.app/profile/winwinkit.bsky.social",
      "x-twitter": "https://x.com/winwin_kit",
      "github": "https://github.com/winwinkit"
    }
  }
}